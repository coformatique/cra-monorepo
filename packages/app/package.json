		"gulp-inline-source": "4.0.0",
		"gulp-replace": "1.0.0",
		"react-router-dom": "5.2.0",
		"uuidv4": "6.2.6",
		"web-vitals": "0.2.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
